        <div class="right-column">
          <p id="a3-result"></p>
        </div>
      </div>
    </div>
  </section>
</div>

<!-- UI WRAPPER -->
<section id="ui-wrapper">
  <div id="left">
    <div id="sanity-container">
      <p>SANITY: <span id="sanity-level"></span> / 10</p>
    </div>
    <div id="moon-container">
      <p>MOON: <span id="moon-level"></span> / 25</p>
    </div>
  </div>
  <div id="right">
    <p>MESSAGE LOG:</p>
    <div id="message-log"></div>
  </div>

</section>
