  <!-- CARD WRAPPER -->
  <section id="card-wrapper">
    <h1 id="room-type"></h1>
    <div class="card-element-container">
      <div>
        <h2 class="card-name"></h2>
        <h3 class="flavor-text"></h3>
      </div>
      <div>
        <p id="card-outlook"></p>
      </div>
    </div>
    <div id="fate-cards-in-play"></div>
    <div id="meet-your-fate-container"></div>
    <div id="actions-wrapper">
      <div class="card-element-container select" id="action-1">
        <div class="left-column">
          <h2 id="a1-name"></h2>
        </div>
        <div class="right-column">
          <p id="a1-fortune"></p>
          <p id="a1-hardship"></p>
        </div>
      </div>
      <div class="card-element-container select" id="action-2">
        <div class="left-column">
          <h2 id="a2-name"></h2>
        </div>
        <div class="right-column">
          <p id="a2-result"></p>
